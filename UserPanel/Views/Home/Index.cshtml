@model IEnumerable<User>

@{
    ViewData["Title"] = "User Panel page";
}

<div class="text-center">
    <h1 class="display-4">User manager</h1>
</div>

<table>
    <tr>
        <th>GUID</th>
        <th>Email</th>
        <th>Registration date</th>
        <th>Last login date</th>
    </tr>
    @foreach(var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Email</td>
            <td>@user.RegDate</td>
            <td>@(user.LastLoginDate == null ? "null" : user.LastLoginDate)</td>
        </tr>
    }
</table>

